{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint","import"],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "import/order": "off"
    },
    "settings": { 
      "import/parsers": {
        "@typescript-eslint/parser": [".ts", ".tsx", ".js"] 
      }, 
      "import/resolver": { 
        "typescript": "./tsconfig.json" 
      }
    }
  }